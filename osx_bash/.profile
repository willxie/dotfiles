# Colored list directory
alias grep='grep --color=auto'

# Emacs Mac Port commandline
alias emacs='open -a /usr/local/Cellar/emacs-mac/emacs-24.5-z-mac-5.8/Emacs.app $1'
alias ec='/usr/local/Cellar/emacs-mac/emacs-24.5-z-mac-5.8/Emacs.app/Contents/MacOS/bin/emacsclient'

# Mac
alias gcc='/usr/local/bin/gcc-6 -std=c++11'
alias g++='/usr/local/bin/g++-6 -std=c++11'
alias gccc='/usr/local/bin/gcc-6 -std=c++11 -Werror -Wall -Wextra'
alias g+++='/usr/local/bin/g++-6 -std=c++11 -Werror -Wall -Wextra'

# Python
alias py='python'
alias jnb='jupyter notebook'

# added by Anaconda2 2.4.1 installer
export PATH="/Users/wxie/anaconda/bin:$PATH"
